{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "*": ["src/*"],
      "@api/*": ["src/api/*"],
      "@assets/*": ["src/assets/*"],
      "@baseComponents/*": ["src/baseComponents/*"],
      "@components/*": ["src/components/*"],
      "@constant/*": ["src/constant/*"],
      "@modules/*": ["src/modules/*"],
      "@navigations/*": ["src/navigations/*"],
      "@store/*": ["src/store/*"],
      "@utils/*": ["src/utils/*"],
      "@services/*": ["src/services/*"]
    },
  }
}
